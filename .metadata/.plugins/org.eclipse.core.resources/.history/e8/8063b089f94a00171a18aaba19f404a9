package adt;

public class Counter {
	
	String str;
	int Counter = 0;
	
	public Counter(String str, int Counter) {
		super();
		this.str = str;
		this.Counter = 0;
	}
	
	int getCurrentValue(){
		return Counter;
	}
	
	int increment(){
		return this.Counter += 1;
	}
	
	String convertToString(){
		return Integer.toString(Counter);
	}
	
	public static void main(String[] args) {
		Counter first = new Counter;
	}
}

