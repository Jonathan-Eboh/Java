package adt;

public class Counter2 {

	private String name = null;
	private int value = 0;
	
	public void Counter2(String str){
		this.name = str;
	}
	
	public void increment(){
		value++;
	}
	
	public int getCurrentValue(){
		return value;
	}
	
	public String toString(){
		return name + ": " + value;
	}
}
